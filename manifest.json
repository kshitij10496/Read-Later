{
	"manifest_version": 6,

	"name": "Read Later",
	"description": "This extension notifies you to view a page later",
	"version": "0.1",
	"icons": {
    	"16": "icon.png",
    	"48": "icon.png",
    	"128": "icon.png"
    },

	"browser_action": {
		"default_title": "ReadLater",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

  "background": {
    "scripts": ["background.js"],
		"persistent": false
	},

	"permissions": [
		"bookmarks",
		"activeTab",
		"alarms",
		"tabs",
		"notifications",
		"storage"
	]
}
